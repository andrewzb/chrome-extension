{
    "key": "<app_id>",
    "manifest_version": 2,
    "name": "PassportExtension",
    "version": "1.0",
    "description": "PassportExtension",
    "icons": {
        "128": "logo.png"
    },
    "browser_action": {
        "default_title": "",
        "default_icon": "logo.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "cookies",
        "<all_urls>",
        "clipboardWrite",
        "clipboardRead",
        "identity",
        "identity.email"
    ],
    "oauth2": {
        "server": "<server_url>",
        "rout": "https://www.googleapis.com/oauth2/v1/userinfo?alt=json&access_token=",
        "client_id": "Goofle_auth_id",
        "scopes": [
            "openid",
            "email",
            "profile"
        ]
    }

}